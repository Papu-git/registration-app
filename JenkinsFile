pipeline {
    agent {
        label 'Jenkins-Agent'
    }

    tools {
        maven 'Maven3.9.6'
        jdk 'Java17'
    }

    stages {
        stage('Clean Workspace') {
            steps {
                cleanWs()
            }
        }
        
        stage('GIT Checkout') {
            steps {
                git branch: 'QA', credentialsId: 'github-accesss', url: 'https://github.com/Papu-git/registration-app.git'
            }
        }
        
        stage('Maven Package') {
            steps {
                sh "mvn package"
            }
        }
        
        stage('Maven Test') {
            steps {
                sh "mvn test"
            }
        }
stage ('Docker login'){
   steps {
 withDockerRegistry(credentialsId: 'Dockerhubcred', url: 'https://index.docker.io/v1/')  {
    // some block
sh 'docker build -t your-image-name .'
sh 'docker push your-image-name'
}

    }
}
}}
