pipeline {
    agent {
        label 'Jenkins-Agent'
    }

    tools {
        maven 'Maven3.9.6'
        jdk 'Java17'
    }

    stages {
        stage('Clean Workspace') {
            steps {
                cleanWs()
            }
        }
        
        stage('GIT Checkout') {
            steps {
                git branch: 'QA', credentialsId: 'github-accesss', url: 'https://github.com/Papu-git/registration-app.git'
            }
        }
        
        stage('maven package'){
            steps{
                sh "mvn package"
         }
      }
   }
}
